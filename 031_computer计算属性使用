<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<title></title>
	<script src="vue.js"></script>
  <script src="vue-router.js"></script>
</head>
<body>
  <div id="app">
    <input type="text" v-model='msga'> + 
    <input type="text" v-model='msgb'> =
    <input type="text" v-model='msgc'>
  </div>

  <script>
  	var vm = new Vue({
  		el : '#app',
  		data : {
        msga : '',
        msgb : ''
  		},
  		methods : {
 
  		},
      computed : {
        msgc : function(){
          return msgc = this.msga + this.msgb;
        }
      }
  	})
  </script>
</body>
